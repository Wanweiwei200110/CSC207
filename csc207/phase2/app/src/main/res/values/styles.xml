<resources>

    <!-- Base application theme. -->
    <style name="Moon" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:textColor">@color/colorMoon</item>
    </style>

    <style name="Mink" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:textColor">@color/colorMink</item>
    </style>

    <style name="Peach" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:textColor">@color/colorPeach</item>
    </style>

    <style name="Orange" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:textColor">@color/colorOrange</item>
    </style>



    <style name="Moon.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Mink.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="GreenText" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#E4F7E4</item>
    </style>

    <style name="customfontstyle" parent="@android:style/TextAppearance.Small">
    </style>




</resources>
